        tau = 1e-3
        V_g = 220 * np.sqrt(2)
        V_rms = 220
        F = 50
        
        L = 1e-3
        R = L / tau
        
        I_amp = 10
        I_angle_deg = 0
        I_angle_rad = np.deg2rad(I_angle_deg)
        
        I_x = I_amp * np.cos(I_angle_rad)
        I_y = I_amp * np.sin(I_angle_rad)
        
